{
  "expo": {
    "name": "SignBridge",
    "slug": "signbridge",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "primaryColor": "#00FF88",
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*",
      "assets/Modelo/**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.signbridge.app",
      "infoPlist": {
        "NSCameraUsageDescription": "SignBridge necesita acceso a la c치mara para detectar letras del alfabeto de se침as."
      }
    },
    "android": {
      "package": "com.signbridge.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "SignBridge necesita acceso a la c치mara para detectar letras del alfabeto de se침as."
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "2.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "132440fa-b9a7-4abd-9fc5-80920cfb94be"
      }
    },
    "owner": "seedbastian"
  }
}
